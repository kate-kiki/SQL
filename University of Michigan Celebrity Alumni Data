/* Create table about the people and what they do here */

/*University of Michigan Celebrity Alumni Hall of Fame*/

CREATE TABLE umich_alumni (id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT,
    age INTEGER,
    year INTEGER,
    major TEXT,
    career TEXT);
    
