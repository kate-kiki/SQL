/*What did the most alumni major in?*/
SELECT major, COUNT(name) 
    FROM umich_alumni 
    GROUP BY major
    ORDER BY COUNT(name) desc;
    
/*How many famous UMich Alumni actually never graduated from UMich?*/
SELECT COUNT(name)
    FROM umich_alumni
    WHERE major = "None";
  
 


